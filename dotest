cd test/
 
for i in *.c
do
   echo "$i" + " compile"  
   ../minic "$i" 
done

  echo "*------------------------------------------------------------------------*"
  echo "*-----------------------------execution---------------------------*" 
  echo "*-------------------------------------------------------------------*"
   
for i in *.s
do
   echo "$i" + " execute"
   echo "*---------------------------------------------------------------*"   
   java -jar Mars_4_2.jar "$i"
done
